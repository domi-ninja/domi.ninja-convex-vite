{
  "name": "flex-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite --open --host 0.0.0.0 --port 5000",
    "dev:backend": "convex dev",
    "build": "tsc -p convex -noEmit --pretty false && tsc -p . -noEmit --pretty false && convex dev --once && vite build",
    "start": "vite preview --host 0.0.0.0 --strictPort --port ${PORT:-4173}",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc -p convex -noEmit && tsc -p . -noEmit"
  },
  "dependencies": {
    "@auth/core": "^0.40.0",
    "@convex-dev/auth": "^0.0.80",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
